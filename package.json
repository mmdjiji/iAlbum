{
  "name": "ialbum",
  "version": "1.1.0",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build && node build.js",
    "build:vue": "vue-cli-service build",
    "build:album": "node build.js",
    "lint": "vue-cli-service lint"
